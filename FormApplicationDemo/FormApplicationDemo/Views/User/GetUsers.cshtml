@model List<UserModel>;
<table>
    <thead>
        <tr>
            <td>First Name</td>
        <td>Last Name</td>
        <td>Age</td>
        </tr>
        
    </thead>
    <tbody>
        
@foreach(var user in Model){
    <tr>
        <td>@user.FirstName</td>
        <td>@user.LastName</td>
        <td>@user.Age</td>
    </tr>
     
    
    
}
</tbody>
</table>
